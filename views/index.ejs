<% include partials/header %>
<% blog.forEach((blog) => { %>
    <h1><%=blog.title%></h1>
    <img src="<%=blog.image%>" alt="blog image">
    <span><%= blog.created.toDateString() %></span>
    <p><%= blog.body.substring(0, 100)%>...</p>
    <a href="/blogs/<%=blog._id%>">Read more</a>
<% }) %>
<% include partials/footer %>

